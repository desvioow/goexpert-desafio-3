# GraphQL schema example
#
# https://gqlgen.com/getting-started/

type Query {
  order(id: String!): Order
  orders: [Order]
}

type Order {
  id: String!
  Price: Float!
  Tax: Float!
  FinalPrice: Float!
}

input OrderInput {
  id : String!
  Price: Float!
  Tax: Float!
}

type Mutation {
  createOrder(input: OrderInput): Order
}